basicTest Example.idr << EOF
    evalEnv @{Additive} natEnv eg
    evalEnv @{Multiplicative} natEnv eg

    evalEnv listEnv eg

    evalEnv @{And} boolEnv eg
    evalEnv @{Or} boolEnv eg
    evalEnv @{Xor} boolEnv eg

    evalEnv listEnv e
    evalEnv listEnv $ eg * eg

    :t assoc @{Additive}
    :t leftId @{Xor}
    :t rightId {a = List String}
EOF
